CREATE TABLE wash_inputs (
	input_id varchar(255) PRIMARY KEY,
	input_name varchar(255) NOT NULL,
	input_type varchar(255) NOT NULL,
	input_price float(10,2) NOT NULL,
	input_make varchar(255)
);

